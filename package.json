{
  "name": "inertify-nest-vue",
  "version": "0.1.0",
  "description": "A demo app designed to help illustrate how Inertia.js works with Nest.js and Vue 3.",
  "author": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist && npm run db:generate && npm run db:migrate:deploy && npm run db:seed",
    "build": "nest build && cd client && vite build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "NODE_ENV=production node dist/main.js",
    "dev": "NODE_ENV=development concurrently \"npm:start:debug\" \"npm:start:client\" -c \"blue,green\" -k",
    "start:client": "cd client && vite",
    "start:server": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "db:seed": "prisma db seed",
    "db:push": "prisma db push",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy"
  },
  "dependencies": {
    "@headlessui/vue": "^1.4.1",
    "@inertiajs/inertia": "^0.10.1",
    "@inertiajs/inertia-vue3": "^0.5.2",
    "@inertiajs/progress": "^0.2.6",
    "@inertify/core": "0.1.0",
    "@inertify/express": "0.1.0",
    "@inertify/express-flash": "0.1.0",
    "@inertify/nest": "0.1.0",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^1.1.5",
    "@nestjs/core": "^8.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/platform-fastify": "^8.2.3",
    "@prisma/client": "^3.5.0",
    "@vueuse/core": "^6.7.3",
    "autoprefixer": "^10.4.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "concurrently": "^6.4.0",
    "connect-redis": "^6.0.0",
    "ejs": "^3.1.6",
    "express-session": "^1.17.2",
    "faker": "^5.5.3",
    "joi": "^17.4.2",
    "multer": "^1.4.4",
    "passport": "^0.5.0",
    "passport-local": "^1.0.0",
    "prisma": "^3.5.0",
    "prisma-pagination": "^0.2.3",
    "redis": "^3.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "vue": "^3.2.20"
  },
  "devDependencies": {
    "@iconify-json/heroicons-outline": "^1.0.1",
    "@iconify-json/heroicons-solid": "^1.0.1",
    "@iconify-json/icon-park-outline": "^1.0.1",
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@tailwindcss/forms": "^0.4.0-alpha.2",
    "@tailwindcss/typography": "^0.5.0-alpha.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/connect-redis": "^0.0.17",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/faker": "^5.5.9",
    "@types/jest": "^27.0.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.0.0",
    "@types/passport-local": "^1.0.34",
    "@types/redis": "^2.8.32",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vitejs/plugin-vue": "^1.9.3",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.5",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "tailwindcss": "^3.0.1",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5",
    "unplugin-auto-import": "^0.4.12",
    "unplugin-icons": "^0.12.16",
    "unplugin-vue-components": "^0.15.6",
    "vite": "^2.6.5",
    "vite-svg-loader": "^2.2.0"
  },
  "prisma": {
    "seed": "ts-node --files prisma/seed.ts"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}